                                    <td></td>
                                    <td class="box">@Html.Raw((Model.Tooth71 != null && Model.Tooth71.Condition != null ? Model.Tooth71.Condition.Abbreviation : ""))</td>
                                    <td class="box">@Html.Raw((Model.Tooth72 != null && Model.Tooth72.Condition != null ? Model.Tooth72.Condition.Abbreviation : ""))</td>
                                    <td class="box">@Html.Raw((Model.Tooth73 != null && Model.Tooth73.Condition != null ? Model.Tooth73.Condition.Abbreviation : ""))</td>
                                    <td class="box">@Html.Raw((Model.Tooth74 != null && Model.Tooth74.Condition != null ? Model.Tooth74.Condition.Abbreviation : ""))</td>
                                    <td class="box">@Html.Raw((Model.Tooth75 != null && Model.Tooth75.Condition != null ? Model.Tooth75.Condition.Abbreviation : ""))</td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="table-responsive-xl mx-5 mb-5">
                        <table class="table table-bordered" id="dental-chart">
                            <tr>
                                <td>&#10003;</td>
                                <td>Present</td>
                                <td>Un</td>
                                <td>Unerupted</td>
                                <td>Att</td>
                                <td>Attachment</td>
                                <td><div class="border p-2" style="background-color: lightgreen"></div></td>
                                <td>Inlay</td>
                            </tr>
                            <tr>
                                <td>M</td>
                                <td>Missing Due To Caries</td>
                                <td>X</td>
                                <td>Extraction Due To Caries</td>
                                <td>P</td>
                                <td>Pontic</td>
                                <td><div class="border p-2" style="background-color: purple"></div></td>
                                <td>Onlay</td>
                            </tr>
                            <tr>
                                <td>Mo</td>
                                <td>Missing Due To Other Causes</td>
                                <td>Xo</td>
                                <td>Extraction Due To Other Causes</td>
                                <td>Imp</td>
                                <td>Implant</td>
                                <td><div class="border p-2" style="background-color: darkblue"></div></td>
                                <td>Sealant</td>
                            </tr>
                            <tr>
                                <td>Im</td>
                                <td>Impacted Tooth</td>
                                <td>JC</td>
                                <td>Jacket Crown</td>
                                <td><div class="border p-2" style="background-color: darkgray"></div></td>
                                <td>Amalgam</td>
                                <td><div class="border p-2" style="background-color: red"></div></td>
                                <td>Caries</td>
                            </tr>
                            <tr>
                                <td>Rf</td>
                                <td>Root Fragment</td>
                                <td>Ab</td>
                                <td>Abutment</td>
                                <td><div class="border p-2" style="background-color: blue"></div></td>
                                <td>Composite</td>
                                <td></td>
                                <td></td>
                            </tr>
                        </table>
                    </div>

                    

                    <!--Tooth Form Modal-->
                    <div class="modal fade" id="toothFromModal" tabindex="-1" aria-labelledby="toothFromModalLabel" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h3 class="modal-title fs-24" id="toothFromModalLabel"></h3>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                </div>
                                <div class="modal-body" id="toothFromModalBodyDiv">
                                </div>
                            </div>
                        </div>
                    </div>

                    <script src="~/js/dental-chart.js"></script>

                    <hr />

                    <h2 class="mb-3 fw-bold fs-24">Oral Cavity Conditions</h2>

                    @if (Model.DentalChart.Status == "Completed" || Model.DentalChart.IsArchived == true)
                    {
                        <ul list-unstyled mb-0>
                            @(Model.DentalChart.OralCavity.IsGingivitis == true ? @Html.Raw("<li>Gingivitis</li>") : null)
                            @(Model.DentalChart.OralCavity.IsEarlyPerio == true ? @Html.Raw("<li>Early Periodontitis</li>") : null)
                            @(Model.DentalChart.OralCavity.IsModPerio == true ? @Html.Raw("<li>Moderate Periodontitis</li>") : null)
                            @(Model.DentalChart.OralCavity.IsAdvPerio == true ? @Html.Raw("<li>Advanced Periodontitis</li>") : null)
                            @(Model.DentalChart.OralCavity.IsOverjet == true ? @Html.Raw("<li>Overjet</li>") : null)
                            @(Model.DentalChart.OralCavity.IsOverbite == true ? @Html.Raw("<li>Overbite</li>") : null)
                            @(Model.DentalChart.OralCavity.IsMidlineDeviation == true ? @Html.Raw("<li>Midline Deviation</li>") : null)
                            @(Model.DentalChart.OralCavity.IsCrossbite == true ? @Html.Raw("<li>Crossbite</li>") : null)
                            @(Model.DentalChart.OralCavity.IsClenching == true ? @Html.Raw("<li>Clenching</li>") : null)
                            @(Model.DentalChart.OralCavity.IsClicking == true ? @Html.Raw("<li>Clicking</li>") : null)
                            @(Model.DentalChart.OralCavity.IsTrismus == true ? @Html.Raw("<li>Trismus</li>") : null)
                            @(Model.DentalChart.OralCavity.IsMusclePain == true ? @Html.Raw("<li>Muscle Pain</li>") : null)
                        </ul>

                        <div class="row">
                                <div class="col-sm-3">
                                    <div class="mb-3">
                                        <label class="form-label" for="DentalChart_OralCavity_OrthoApplication">Orthodontic Application</label>
                                        <input asp-for="@Model.DentalChart.OralCavity.OrthoApplication" class="form-control" disabled/>
                                        <span asp-validation-for="@Model.DentalChart.OralCavity.OrthoApplication" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="mb-3">
                                        <label class="form-label" for="DentalChart_OralCavity_ClassType">Class Type</label>
                                        <input asp-for="@Model.DentalChart.OralCavity.ClassType" class="form-control" disabled/>
                                        <span asp-validation-for="@Model.DentalChart.OralCavity.ClassType" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                    }
                    else
                    {
                        <form asp-action="UpdateOralCavity" method="post">
                            <input type="hidden" asp-for="@Model.DentalChart.OralCavity.Id" />
                            <input type="hidden" asp-for="@Model.DentalChart.Id" />
                            <div class="row mb-3">
                                <div class="col-md-3">
                                    <!--Gingivitis-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsGingivitis">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsGingivitis"></label>
                                    </div>
                                    <!--Early Periodontitis-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsEarlyPerio">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsEarlyPerio"></label>
                                    </div>
                                    <!--Moderate Periodontitis-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsModPerio">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsModPerio"></label>
                                    </div>
                                    <!--Advanced Periodontitis-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsAdvPerio">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsAdvPerio"></label>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <!--Overjet-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsOverjet">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsOverjet"></label>
                                    </div>
                                    <!--Overbite-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsOverbite">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsOverbite"></label>
                                    </div>
                                    <!--Midline Deviation-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsMidlineDeviation">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsMidlineDeviation"></label>
                                    </div>
                                    <!--Crossbite-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsCrossbite">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsCrossbite"></label>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <!--Clenching-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsClenching">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsClenching"></label>
                                    </div>

                                    <!--Clicking-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsClicking">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsClicking"></label>
                                    </div>

                                    <!--Trismus-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsTrismus">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsTrismus"></label>
                                    </div>

                                    <!--Muscle Pain-->
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" asp-for="@Model.DentalChart.OralCavity.IsMusclePain">
                                        <label class="form-check-label" asp-for="@Model.DentalChart.OralCavity.IsMusclePain"></label>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="mb-3">
                                        <label class="form-label" for="DentalChart_OralCavity_OrthoApplication">Orthodontic Application</label>
                                        <input asp-for="@Model.DentalChart.OralCavity.OrthoApplication" class="form-control" />
                                        <span asp-validation-for="@Model.DentalChart.OralCavity.OrthoApplication" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="mb-3">
                                        <label class="form-label" for="DentalChart_OralCavity_ClassType">Class Type</label>
                                        <input asp-for="@Model.DentalChart.OralCavity.ClassType" class="form-control" />
                                        <span asp-validation-for="@Model.DentalChart.OralCavity.ClassType" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            @if (Model.DentalChart.Status == "Draft" && Model.DentalChart.IsArchived == false)
                            {
                                <button type="submit" class="btn btn-primary"><i class="fas fa-save me-2"></i>Save</button>
                            }
                        </form>
                    }

                    <div class="d-flex flex-wrap mt-3">
                        @if (Model.DentalChart.Status == "Completed" || Model.DentalChart.IsArchived == true)
                        {
                            <a asp-action="Index" asp-route-id="@patientId" class="btn btn-secondary me-2"><i class="fas fa-arrow-left me-2"></i>Back</a>
                        }
                        else
                        {
                            <form asp-action="Finish" method="post">
                                <input type="hidden" name="dentalChartId" value="@Model.DentalChart.Id" />
                                <button type="submit" class="btn btn-primary me-2"><i class="fas fa-check me-2"></i>Finish Dental Chart</button>
                            </form>
                            <a asp-action="Index" asp-route-id="@patientId" class="btn btn-secondary me-2"><i class="fas fa-arrow-left me-2"></i>Back</a>
                        }
                    </div>


                </div>
            </div>
        }
    }
}