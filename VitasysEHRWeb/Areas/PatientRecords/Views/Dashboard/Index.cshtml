<div class="row gx-3 mt-3">

    <div class="col-lg-4 mb-3">
        <div class="card shadow-sm h-100 rounded-3">
            <div class="card-body">

                <div class="d-flex justify-content-between">
                    <h2 class="m-0 fw-bold">Prescriptions</h2>
                    @if (Model.Prescriptions.Count > 0)
                    {
                        <div>
                            <a asp-area="PatientRecords" asp-controller="Prescription" asp-action="Index" 
                                asp-route-id="@Model.PatientId" class="btn btn-primary rounded-3">
                                <i class="fas fa-search-plus me-2"></i>See All
                            </a>
                        </div>
                    }
                </div>

                @if (Model.Prescriptions.Count > 0)
                {
                    <table class="table table-striped table-borderless mt-3 border">
                        <thead class="text-muted border-bottom">
                            <tr>
                                <th style="width:30px">#</th>
                                <th>Recent Prescriptions</th>
                                <th>Quantity</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (var i = 0; i < Model.Prescriptions.Count; i++)
                            {
                                if (i == 4) break;

                                <tr>
                                    <td>@(i + 1)</td>
                                    <td>@Model.Prescriptions[i].Drug @Model.Prescriptions[i].Dose @Model.Prescriptions[i].Dosage</td>
                                    <td>@Model.Prescriptions[i].Quantity</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
                else
                {
                    <div class="text-center py-2 mt-3">
                        <i class="fas fa-pills d-flex justify-content-center mb-3" style="font-size:40px"></i>
                        <h3>No prescriptions</h3>
                        <p>Start adding prescriptions by clicking the button below.</p>
                        <a asp-area="PatientRecords" asp-controller="Prescription" asp-action="Index" 
                            asp-route-id="@Model.PatientId" class="btn btn-success rounded-3">
                            <i class="fas fa-plus me-1"></i> Add Prescription
                        </a>
                    </div>
                }

            </div>
        </div>
    </div>

    @if (subscriptionIsLockout == "false")
    {
        @if (subscriptionType == "Basic" || subscriptionType == "Pro")
        {
             <div class="col-lg-4 mb-3">
                <div class="card shadow-sm h-100 rounded-3">
                    <div class="card-body">

                        <div class="d-flex justify-content-between">
                            <h2 class="m-0 fw-bold">Documents</h2>
                            @if (Model.Documents.Count > 0)
                            {
                                <div>
                                    <a asp-area="PatientRecords" asp-controller="Document" asp-action="Index" 
                                        asp-route-id="@Model.PatientId" class="btn btn-primary rounded-3">
                                        <i class="fas fa-search-plus me-2"></i>See All
                                    </a>
                                </div>
                            }
                        </div>

                        @if (Model.Documents.Count > 0)
                        {
                            <table class="table table-striped table-borderless mt-3 border">
                                <thead class="text-muted border-bottom">
                                    <tr>
                                        <th>#</th>
                                        <th>Recent Documents</th>
                                        <th>From Folder</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (var i = 0; i < Model.Documents.Count; i++)
                                    {
                                        if (i == 4) break;

                                        <tr>
                                            <td>@(i + 1)</td>
                                            <td>@Model.Documents[i].Name</td>
                                            <td>@Model.Documents[i].Folder.Name.Substring(0, Model.Documents[i].Folder.Name.Length - 33)</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        }
                        else
                        {
                            <div class="text-center py-2 mt-3">
                                <i class="far fa-folder-open d-flex justify-content-center mb-3" style="font-size:40px"></i>
                                <h3>No documents</h3>
                                <p>Start adding documents by clicking the button below.</p>
                                <a asp-area="PatientRecords" asp-controller="Document" asp-action="Index" 
                                    asp-route-id="@Model.PatientId" class="btn btn-success rounded-3">
                                    <i class="fas fa-plus me-1"></i> Add Documents
                                </a>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="col-lg-4 mb-3">
                <div class="card shadow-sm h-100 rounded-3">
                    <div class="card-body">
                        <div class="d-flex justify-content-between">
                            <h2 class="m-0 fw-bold">Reminders</h2>
                            @if (Model.Reminders.Count > 0)
                            {
                                <div>
                                    <a asp-area="PatientRecords" asp-controller="Reminder" asp-action="Index" 
                                        asp-route-id="@Model.PatientId" class="btn btn-primary rounded-3">
                                        <i class="fas fa-search-plus me-2"></i>See All
                                    </a>
                                </div>
                            }
                        </div>

                        @if (Model.Reminders.Count > 0)
                        {
                            <table class="table table-borderless mt-3 table-striped border">
                                <thead class="text-muted border-bottom">
                                    <tr>
                                        <th>#</th>
                                        <th>Recent Reminders</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (var i = 0; i < Model.Reminders.Count; i++)
                                    {
                                        if (i == 4) break;

                                        <tr>
                                            <td>@(i + 1)</td>
                                            <td>@Model.Reminders[i].Title</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        }
                        else
                        {
                            <div class="text-center py-2 mt-3">
                                <i class="fas fa-bell d-flex justify-content-center mb-3" style="font-size:40px"></i>
                                <h3>No reminders</h3>
                                <p>Start adding reminders by clicking the button below.</p>
                                <a asp-area="PatientRecords" asp-controller="Reminder" asp-action="Index" 
                                    asp-route-id="@Model.PatientId" class="btn btn-success rounded-3">
                                    <i class="fas fa-plus me-1"></i> Add Reminder
                                </a>
                            </div>
                        }
                    </div>
                </div>
            </div>   
        }
    }
    
</div>