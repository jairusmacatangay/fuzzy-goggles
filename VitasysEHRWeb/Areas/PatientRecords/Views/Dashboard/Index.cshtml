@model VitasysEHR.Models.ViewModels.DashboardVM
@using VitasysEHR.Utility
@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

@{
    Layout = "_SideNavLayout";
    ViewData["Title"] = "Patient Records - Dashboard";

    string? subscriptionType = HttpContextAccessor.HttpContext.Session.GetString(SD.SessionKeySubscriptionType);
    string? subscriptionIsLockout = HttpContextAccessor.HttpContext.Session.GetString(SD.SessionKeySubscriptionIsLockout);
}

<div class="d-flex align-items-center mb-3 mt-3">
    <i class="fas fa-columns me-2" style="font-size: 2rem;"></i>
    <h1 class="fw-bolder m-0">Dashboard</h1>
</div>


<div class="row gx-3">
    <div class="col-lg-6 mb-3">
        <div class="card shadow-sm h-100 rounded-3">
            <div class="card-body">

                <div class="d-flex justify-content-between">
                    <h2 class="m-0 fw-bold">Medical History</h2>
                    @if (Model.MedicalHistoryVM.MedicalHistory != null)
                    {
                        <div>
                            <a asp-area="PatientRecords" asp-controller="MedicalHistory" asp-action="Index"
                                asp-route-id="@Model.PatientId" class="btn btn-primary rounded-3">
                                <i class="fas fa-search-plus me-2"></i>See More
                            </a>
                        </div>
                    }
                </div>

                @if (Model.MedicalHistoryVM.MedicalHistory != null)
                {
                    <div class="row gx-3 mt-3">
                        <div class="col-sm-4 mb-3">
                            <div class="card bg-light border-light rounded-3">
                                <div class="card-body">
                                    <p class="text-muted fw-bold m-0">Blood Type</p>
                                    @if(Model.MedicalHistoryVM.MedicalHistory.BloodType == null){
                                        <p class="m-0"><br/></p>
                                    } else {
                                        <p class="m-0">@Model.MedicalHistoryVM.MedicalHistory.BloodType</p>
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4 mb-3">
                            <div class="card bg-light border-light rounded-3">
                                <div class="card-body">
                                    <p class="text-muted fw-bold m-0">Blood Pressure</p>
                                    @if(Model.MedicalHistoryVM.MedicalHistory.BloodPressure == null){
                                        <p class="m-0"><br/></p>
                                    } else {
                                        <p class="m-0">@Model.MedicalHistoryVM.MedicalHistory.BloodPressure</p>
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4 mb-3">
                            <div class="card bg-light border-light rounded-3">
                                <div class="card-body">
                                    <p class="text-muted m-0 fw-bold">Bleeding Time</p>
                                    @if(Model.MedicalHistoryVM.MedicalHistory.BleedingTime == null){
                                        <p class="m-0"><br/></p>
                                    } else {
                                        <p class="m-0">@Model.MedicalHistoryVM.MedicalHistory.BleedingTime</p>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row gx-3">
                        <div class="col-sm-6 mb-3">
                            <div class="card bg-light border-light h-100">
                                <div class="card-body rounded-3">
                                    <p class="text-muted m-0 fw-bold">Medical Problems</p>
                                    <ul class="list-unstyled mb-0" id="medicalHistoryUl">
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHbp == true ? @Html.Raw("<li>High Blood Pressure</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsLbp == true ? @Html.Raw("<li>Low Blood Pressure</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsEpilepsy == true ? @Html.Raw("<li>Epilepsy / Convulsions</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsAids == true ? @Html.Raw("<li>AIDs or HIV Infection</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsStd == true ? @Html.Raw("<li>Sexually Transmitted Disease</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsStomach == true ? @Html.Raw("<li>Stomach Troubles / Ulcers</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsFainting == true ? @Html.Raw("<li>Fainting / Seizure</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsWeightLoss == true ? @Html.Raw("<li>Rapid Weight Loss</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsRadiation == true ? @Html.Raw("<li>Radiation Therapy</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsJointReplacement == true ? @Html.Raw("<li>Joint Replacement / Implant</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHeartSurgery == true ? @Html.Raw("<li>Heart Surgery</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHeartAttack == true ? @Html.Raw("<li>Heart Attack</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsThyroid == true ? @Html.Raw("<li>Thyroid Problem</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHeartDisease == true ? @Html.Raw("<li>Heart Disease</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHeartMurmur == true ? @Html.Raw("<li>Heart Murmur</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHepa == true ? @Html.Raw("<li>Hepatitis / Liver Disease</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsRheumatic == true ? @Html.Raw("<li>Rheumatic Fever</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHayFever == true ? @Html.Raw("<li>Hay Fever / Allergies</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsRespiratory == true ? @Html.Raw("<li>Respiratory Problems</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsJaundice == true ? @Html.Raw("<li>Hepatitis / Jaundice</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsTuberculosis == true ? @Html.Raw("<li>Tuberculosis</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsSwollenAnkles == true ? @Html.Raw("<li>Swollen Ankles</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsKidney == true ? @Html.Raw("<li>Kidney Disease</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsDiabetes == true ? @Html.Raw("<li>Diabetes</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsChestPain == true ? @Html.Raw("<li>Chest Pain</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsStroke == true ? @Html.Raw("<li>Stroke</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsCancer == true ? @Html.Raw("<li>Cancer / Tumors</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsAnemia == true ? @Html.Raw("<li>Anemia</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsAngina == true ? @Html.Raw("<li>Angina</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsAsthma == true ? @Html.Raw("<li>Asthma</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsEmphysema == true ? @Html.Raw("<li>Emphysema</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsBleeding == true ? @Html.Raw("<li>Bleeding Problems</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsBloodDisease == true ? @Html.Raw("<li>Blood Diseases</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsHeadInjury == true ? @Html.Raw("<li>Head Injury</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.IsArthritis == true ? @Html.Raw("<li>Arthritis / Rheumatism</li>") : "")
                                        @(Model.MedicalHistoryVM.ReviewOfSystem.Other != null  ? @Html.Raw("<li>" + Model.MedicalHistoryVM.ReviewOfSystem.Other + "</li>") : "")
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 mb-3">
                            <div class="card bg-light border-light h-100">
                                <div class="card-body rounded-3">
                                    <p class="text-muted fw-bold m-0">Allergies</p>
                                    <ul class="list-unstyled mb-0" id="allergyUl">
                                        @(Model.MedicalHistoryVM.Allergy.IsAnesthesia == true ? @Html.Raw("<li>Local Anesthetic</li>") : null)
                                        @(Model.MedicalHistoryVM.Allergy.IsAntibiotics == true ? @Html.Raw("<li>Penicillin, Antibiotics</li>") : null)
                                        @(Model.MedicalHistoryVM.Allergy.IsSulfa == true ? @Html.Raw("<li>Sulfa Drugs</li>") : null)
                                        @(Model.MedicalHistoryVM.Allergy.IsAspirin == true ? @Html.Raw("<li>Aspirin</li>") : null)
                                        @(Model.MedicalHistoryVM.Allergy.IsLatex == true ? @Html.Raw("<li>Latex</li>") : null)
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                    <script defer>
                        const allergyUl = document.getElementById("allergyUl")
                        const medicalHistoryUl = document.getElementById("medicalHistoryUl")

                        if (allergyUl.innerHTML.trim().length == 0) {
                            allergyUl.innerHTML = "<li>The patient has no known allergies.</li>"
                        }

                        if (medicalHistoryUl.innerHTML.trim().length == 0) {
                            medicalHistoryUl.innerHTML = "<li>The patient has no illness or underlying medical conditions.</li>"
                        }
                    </script>
                }
                else
                {
                    <div class="text-center py-2 mt-3">
                        <i class="fas fa-notes-medical d-flex justify-content-center mb-3" style="font-size:40px"></i>
                        <h3>No medical history</h3>
                        <p>Add a medical history by clicking the button below.</p>
                        <a asp-area="PatientRecords" asp-controller="MedicalHistory" asp-action="Index"
                            asp-route-id="@Model.PatientId" class="btn btn-success rounded-3">
                            <i class="fas fa-plus me-1"></i> Add Medical History
                        </a>
                    </div>
                }

            </div>
        </div>
    </div>

    <div class="col-lg-6 mb-3">
        <div class="card shadow-sm h-100 rounded-3">
            <div class="card-body">

                <div class="d-flex justify-content-between">
                    <h2 class="m-0 fw-bold">Treatment Records</h2>
                    @if (Model.TreatmentRecords.Count > 0)
                    {
                        <div>
                            <a asp-area="PatientRecords" asp-controller="Treatment" asp-action="Index" 
                                asp-route-id="@Model.PatientId" class="btn btn-primary rounded-3">
                                <i class="fas fa-search-plus me-2"></i>See All
                            </a>
                        </div>
                    }
                </div>

                @if (Model.TreatmentRecords.Count > 0)
                {
                    <table class="table table-striped table-borderless mt-3 border">
                        <thead class="text-muted border-bottom">
                            <tr>
                                <th>#</th>
                                <th>Recent Treatment</th>
                                <th>Tooth Number/s</th>
                                <th>Dentist/s</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (var i = 0; i < Model.TreatmentRecords.Count; i++)
                            {
                                if (i == 4) break;

                                <tr>
                                    <td>@(i + 1)</td>
                                    <td>@Model.TreatmentRecords[i].Treatment.Name</td>
                                    <td>@Model.TreatmentRecords[i].ToothNumbers</td>
                                    <td>@Model.TreatmentRecords[i].Dentists</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
                else
                {
                    <div class="text-center py-2 mt-3">
                        <i class="fas fa-file-medical d-flex justify-content-center mb-3" style="font-size:40px"></i>
                        <h3>No treatment records</h3>
                        <p>Start adding treatment records by clicking the button below.</p>
                        <a asp-area="PatientRecords" asp-controller="Treatment" asp-action="Index" 
                            asp-route-id="@Model.PatientId" class="btn btn-success rounded-3">
                            <i class="fas fa-plus me-1"></i> Add Treatment
                        </a>
                    </div>
                }


            </div>
        </div>
    </div>
</div>

